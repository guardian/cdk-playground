{
  "name": "@guardian/cdk-playground",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "private": true,
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "format": "prettier --write \"{lib,bin}/**/*.ts\"",
    "synth": "cdk synth --path-metadata false --version-reporting false",
    "lint": "eslint lib/** bin/** --ext .ts --no-error-on-unmatched-pattern",
    "start": "jest --watch"
  },
  "devDependencies": {
    "@guardian/cdk": "41.1.0",
    "@guardian/eslint-config-typescript": "^1.0.1",
    "@types/jest": "^27.5.0",
    "@types/node": "17.0.31",
    "aws-cdk": "2.20.0",
    "aws-cdk-lib": "2.20.0",
    "constructs": "10.0.110",
    "eslint": "^8.14.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "source-map-support": "^0.5.21",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "~4.6.4"
  }
}
