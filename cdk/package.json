{
  "name": "cdk",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "format": "prettier --write \"{lib,bin}/**/*.ts\"",
    "lint": "gu-scripts lint",
    "synth": "cdk synth --path-metadata false --version-reporting false",
    "diff": "cdk diff --path-metadata false --version-reporting false"
  },
  "devDependencies": {
    "@guardian/cdk": "^47.3.3",
    "@guardian/prettier": "2.1.5",
    "@guardian/scripts": "github:guardian/scripts#main",
    "@types/jest": "^28.1.6",
    "@types/node": "18.11.9",
    "aws-cdk": "2.39.1",
    "aws-cdk-lib": "2.39.1",
    "constructs": "10.1.91",
    "jest": "^28.1.3",
    "source-map-support": "^0.5.20",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.3"
  },
  "prettier": "@guardian/prettier",
  "jest": {
    "testMatch": [
      "<rootDir>/lib/**/*.test.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ]
  }
}
