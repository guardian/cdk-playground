{
  "name": "@guardian/cdk-playground",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "private": true,
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "format": "prettier --write \"{lib,bin}/**/*.ts\"",
    "synth": "cdk synth --path-metadata false --version-reporting false",
    "lint": "eslint lib/** bin/** --ext .ts --no-error-on-unmatched-pattern",
    "start": "jest --watch"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.107.0",
    "@guardian/eslint-config-typescript": "^0.6.0",
    "@types/jest": "^26.0.24",
    "@types/node": "16.3.3",
    "aws-cdk": "1.107.0",
    "eslint": "^7.30.0",
    "jest": "^26.6.3",
    "prettier": "^2.3.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.1.0",
    "typescript": "~4.3.5"
  },
  "dependencies": {
    "@aws-cdk/core": "1.107.0",
    "@guardian/cdk": "18.0.0",
    "source-map-support": "^0.5.16"
  }
}
