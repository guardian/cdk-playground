{
  "name": "@guardian/cdk-playground",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "private": true,
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "format": "prettier --write \"{lib,bin}/**/*.ts\"",
    "synth": "cdk synth --path-metadata false --version-reporting false",
    "lint": "eslint lib/** bin/** --ext .ts --no-error-on-unmatched-pattern",
    "start": "jest --watch"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.137.0",
    "@guardian/eslint-config-typescript": "^0.7.0",
    "@types/jest": "^27.4.0",
    "@types/node": "17.0.8",
    "aws-cdk": "1.137.0",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "prettier": "^2.5.1",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.4.0",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "@aws-cdk/core": "1.137.0",
    "@guardian/cdk": "34.0.2",
    "source-map-support": "^0.5.21"
  }
}
