{
  "name": "@guardian/cdk-playground",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "private": true,
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "format": "prettier --write \"{lib,bin}/**/*.ts\"",
    "synth": "cdk synth --path-metadata false --version-reporting false",
    "lint": "eslint lib/** bin/** --ext .ts --no-error-on-unmatched-pattern",
    "start": "jest --watch"
  },
  "devDependencies": {
    "@guardian/cdk": "45.0.0",
    "@guardian/eslint-config-typescript": "^1.0.1",
    "@types/jest": "^27.5.1",
    "@types/node": "17.0.35",
    "aws-cdk": "2.25.0",
    "aws-cdk-lib": "2.25.0",
    "constructs": "10.1.17",
    "eslint": "^8.16.0",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "source-map-support": "^0.5.21",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.8.0",
    "typescript": "~4.7.2"
  }
}
