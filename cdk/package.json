{
  "name": "@guardian/cdk-playground",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "private": true,
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "format": "prettier --write \"{lib,bin}/**/*.ts\"",
    "synth": "cdk synth --path-metadata false --version-reporting false",
    "lint": "eslint lib/** bin/** --ext .ts --no-error-on-unmatched-pattern",
    "start": "jest --watch"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.145.0",
    "@guardian/eslint-config-typescript": "^1.0.1",
    "@types/jest": "^27.4.1",
    "@types/node": "17.0.21",
    "aws-cdk": "1.145.0",
    "eslint": "^8.10.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "~4.6.2"
  },
  "dependencies": {
    "@aws-cdk/core": "1.145.0",
    "@guardian/cdk": "37.0.0",
    "source-map-support": "^0.5.21"
  }
}
